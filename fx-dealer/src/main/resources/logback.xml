<?xml version="1.0" encoding="UTF-8" ?>

<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} %.5level [%.10thread] %c{30} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.hashnot.fx" level="debug" />
    <logger name="com.hashnot.fx.kokos.to" level="warn" />
    <logger name="com.gargoylesoftware.htmlunit.IncorrectnessListenerImpl" level="off" />

    <root>
        <level value="warn" />
        <appender-ref ref="STDOUT" />
    </root>
</configuration>
